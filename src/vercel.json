   {
     "version": 2,
     "builds": [
       { "src": "server.js", "use": "@vercel/node" },
       { "src": "src/main.tsx", "use": "@vercel/static-build" }
     ],
     "routes": [
       { "src": "/api/(.*)", "dest": "server.js" },
       { "src": "/(.*)", "dest": "src/main.tsx" }
     ]
   }
   